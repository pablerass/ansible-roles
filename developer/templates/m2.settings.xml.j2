<settings>
  <mirrors>
    <mirror>
      <id>artifactory</id>
      <mirrorOf>*</mirrorOf>
      <url>{{ repo_url }}/maven</url>
    </mirror>
  </mirrors>
  <servers>
    <server>
      <id>artifactory</id>
      <username>{{ user_name }}</username>
      <password>{{ user_pass }}</password>
    </server>
  </servers>
</settings>